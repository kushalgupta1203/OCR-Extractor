{
    "builds": [
      {
        "src": "live.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "live.py"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "functions": {
      "api/*": {
        "runtime": "python3.9",
        "maxDuration": 10
      }
    },
    "ignoreCommand": "echo 'Ignoring build step to prevent size issues'"
  }
  